{
  "name": "circles",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@scottburch/pistol": "^1.3.0",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-router": "^5.1.20",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "parcel": "^2.8.3",
    "playwright": "^1.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serve": "^14.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "serve": "serve dist",
    "test": "mocha src/**/*.spec.ts",
    "testnet": "node -r ts-node/register --loader ts-node/esm --experimental-specifier-resolution=node src/testnet/testnet.ts"
  },
  "engines": {
    "node": ">=18"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "mocha": {
    "require": "ts-node/register",
    "loader": "ts-node/esm",
    "experimental-specifier-resolution": "node",
    "bail": true,
    "timeout": 100000
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "2.8.3",
    "@parcel/transformer-webmanifest": "2.8.3",
    "buffer": "^5.5.0",
    "process": "^0.11.10"
  }
}
